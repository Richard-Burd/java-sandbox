public class Main {
  public static void main(String[] args) {
    boolean isItTrue = true;
    byte b = 127; // based on the ASCII Table
    char c = 'ÙŠ'; // supports unicode
    short s = -32768; // for really small numbers
    int i = 43; // 32 bit number
    long l = 400000L; // 64 bit character (ends w/ "L")
    float f = 20.5f; // 32 bit
    double d = 201354.3245f; // 64 bit (the "f" is optional)
    // you usually use double instead of float...
    // ...unless you are short on memory for s.reason
    // float is a smaller container that a double.
    float x = (float) (f / d);
  }
}

